<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/NewMemberLayout"
android:layout_width="330dp"
android:layout_height="48dp"
android:layout_marginStart="40dp"
android:layout_marginTop="10dp"
tools:layout_editor_absoluteY="484dp"

    >

    <TextView
        android:id="@+id/textViewMiembros"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/rounded_corners_plain_text"
        android:ems="10"
        android:inputType="text"

        android:padding="10dp"
        android:text="000000000"
        android:textColor="#000000"

        android:textCursorDrawable="@null"
        android:textSize="18dp"
        android:textStyle="bold"
        tools:layout_editor_absoluteY="484dp" />

    <ImageButton
        android:id="@+id/iconMemberButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@id/textViewMiembros"
        android:layout_alignRight="@id/textViewMiembros"
        android:layout_alignBottom="@id/textViewMiembros"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"
        android:layout_marginRight="8dp"
        android:background="@null"
        android:src="@drawable/baseline_group_24"
        android:text="@string/button" />

</RelativeLayout>